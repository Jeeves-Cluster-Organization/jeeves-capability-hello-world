[run]
source = jeeves_capability_hello_world
omit =
    */tests/*
    */test_*.py
    */__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    @abstractmethod
    @abc.abstractmethod
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    pass
    \.\.\.

precision = 2
show_missing = True
fail_under = 80

[html]
directory = htmlcov

[xml]
output = coverage.xml
